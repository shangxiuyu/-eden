{
  "name": "@agentxjs/eden",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Eden - Agent Social Platform powered by AgentX",
  "scripts": {
    "dev": "vite",
    "dev:server": "bun --watch server.ts",
    "dev:all": "concurrently \"bun run dev:server\" \"bun run dev\"",
    "dev:with-gateway": "concurrently \"bun run dev:server\" \"bun run dev\" \"cd ../../../temp_market/agents/openclaw && pnpm run gateway:dev\"",
    "build": "vite build",
    "build:server": "bun run build.ts",
    "start": "NODE_ENV=production bun run dist/server/index.js",
    "typecheck": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@agentxjs/persistence": "workspace:*",
    "@agentxjs/runtime": "workspace:*",
    "@larksuiteoapi/node-sdk": "1.58.0",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-separator": "^1.1.1",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tailwindcss/typography": "0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@types/uuid": "^10.0.0",
    "agentxjs": "workspace:*",
    "axios": "^1.13.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^16.6.1",
    "framer-motion": "^12.29.2",
    "lucide-react": "^0.563.0",
    "openai": "6.16.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-icons": "^5.5.0",
    "react-markdown": "10.1.0",
    "react-router-dom": "^7.13.0",
    "remark-gfm": "4.0.1",
    "tailwind-merge": "^3.4.0",
    "tailwind-variants": "^3.2.2",
    "tailwindcss": "^4.1.18",
    "uuid": "^13.0.0",
    "ws": "^8.18.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/ws": "^8.5.13",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.0",
