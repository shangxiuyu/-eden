{
  "name": "@agentxjs/queue",
  "version": "1.9.0",
  "description": "Reliable event delivery queue for AgentX with SQLite persistence",
  "keywords": [
    "agentx",
    "queue",
    "event",
    "reliable-delivery",
    "sqlite"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/AgentX.git",
    "directory": "packages/queue"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@agentxjs/common": "workspace:*",
    "@agentxjs/types": "workspace:*",
    "rxjs": "^7.8.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
