    "next-themes": "0.4.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "remark-gfm": "^4.0.1",
    "sonner": "2.0.7",
    "tailwind-merge": "^2.5.5",
    "vaul": "^1.1.2",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "16.3.1",
