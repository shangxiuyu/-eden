{
  "name": "@agentxjs/ui",
  "version": "1.9.0",
  "description": "AgentX UI Components - React component library for building AI agent interfaces",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/AgentX.git"
  },
  "homepage": "https://github.com/Deepractice/AgentX",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./globals.css": "./dist/globals.css",
    "./tailwind-preset": "./tailwind.config.js",
    "./dist/agentx-ui.css": "./dist/agentx-ui.css"
  },
  "sideEffects": [
    "*.css",
    "*.scss"
  ],
  "files": [
    "dist",
    "tailwind.config.js",
    "README.md"
