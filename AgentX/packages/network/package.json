{
  "name": "@agentxjs/network",
  "version": "1.9.0",
  "description": "Network layer abstraction for AgentX platform - WebSocket client/server with heartbeat and auto-reconnect",
  "keywords": [
    "agentx",
    "network",
    "websocket",
    "channel",
    "internal"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/AgentX.git",
    "directory": "packages/network"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@agentxjs/common": "workspace:*",
