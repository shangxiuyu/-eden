{
  "name": "@agentxjs/persistence",
  "version": "1.9.0",
  "description": "Multi-backend persistence layer for AgentX - supports memory, SQLite, Redis, MongoDB, and SQL databases",
  "keywords": [
    "agentx",
    "persistence",
    "storage",
    "sqlite",
    "redis",
    "mongodb"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/AgentX.git",
    "directory": "packages/persistence"
  },
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./sqlite": {
      "types": "./dist/drivers/sqlite.d.ts",
      "default": "./dist/drivers/sqlite.js"
    },
    "./redis": {
      "types": "./dist/drivers/redis.d.ts",
      "default": "./dist/drivers/redis.js"
    },
    "./mongodb": {
      "types": "./dist/drivers/mongodb.d.ts",
      "default": "./dist/drivers/mongodb.js"
    },
    "./mysql": {
      "types": "./dist/drivers/mysql.d.ts",
      "default": "./dist/drivers/mysql.js"
    },
    "./postgresql": {
      "types": "./dist/drivers/postgresql.d.ts",
      "default": "./dist/drivers/postgresql.js"
    },
    "./fs": {
      "types": "./dist/drivers/fs.d.ts",
      "default": "./dist/drivers/fs.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@agentxjs/common": "workspace:*",
    "@agentxjs/types": "workspace:*",
    "unstorage": "^1.14.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
