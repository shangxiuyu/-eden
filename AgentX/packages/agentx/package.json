{
  "name": "agentxjs",
  "version": "1.9.0",
  "description": "Unified API for AI Agents - Server and Browser",
  "license": "MIT",
  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/AgentX.git"
  },
  "homepage": "https://github.com/Deepractice/AgentX",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "browser": "./dist/browser.js",
      "node": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun run build.ts",
    "clean": "rm -rf dist test-results",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "dependencies": {
    "@agentxjs/common": "workspace:*",
    "@agentxjs/network": "workspace:*",
    "@agentxjs/types": "workspace:*",
    "reconnecting-websocket": "^4.4.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@agentxjs/persistence": "workspace:*",
    "@agentxjs/queue": "workspace:*",
    "@agentxjs/runtime": "workspace:*"
  },
  "publishConfig": {
    "access": "public"
  }
}
