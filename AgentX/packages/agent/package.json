  "engines": {
    "node": ">=22.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Deepractice/AgentX.git"
  },
  "bugs": {
    "url": "git+https://github.com/Deepractice/AgentX.git"
  },
  "homepage": "https://github.com/Deepractice/Agent#readme",
  "dependencies": {
    "@agentxjs/common": "workspace:*",
    "@agentxjs/types": "workspace:*",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {}
